{
    "manifest_version": 2,
    "name": "Focus 2.0",
    "version": "1.0",
    "description": "Motivates you to work",
    "icons": {
        "48": "CSX-logo.png",
        "96": "CSX-logo.png"
      },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["jquery.min.js", "./main.js"],
          "css": ["style.css"]
        }
      ],
    "web_accessible_resources": [
        "get-back-to-work.mp3"
    ],
    "background": {
      "scripts": ["background.js"]
    },
      
    "browser_action": {
        "default_icon": "CSX-logo.png",
        "default_popup": "popup.html"
    } 
}

